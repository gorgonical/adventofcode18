(require racket/function)

;; This solves part 1
(letrec ([getcounts (lambda (str)
                   (letrec ([chars (string->list str)]
                            [charfreqs (map (lambda (char)
                                              (count ((curry char=?) char) chars))
                                            chars)]
                            [hastwo (positive? (count ((curry =) 2) charfreqs))]
                            [hasthree (positive? (count ((curry =) 3) charfreqs))])
                     (list hastwo hasthree)))]
      [allstrs (map getcounts input)]
      [sumtwos (foldl (lambda (lst acc) (if (first lst) (+ acc 1) acc)) 0 allstrs)]
      [sumthrees (foldl (lambda (lst acc) (if (second lst) (+ acc 1) acc)) 0 allstrs)]
      )
  (* sumtwos sumthrees))

;; This solves part 2
(require levenshtein)

(define (get-pos-different-char str1 str2)
  (letrec ([chars1 (string->list str1)]
           [chars2 (string->list str2)]
           [helper (lambda (c1 c2 count)
                     (if (not (char=? (car c1) (car c2)))
                         count
                         (helper (cdr c1) (cdr c2) (+ 1 count))))])
    (helper chars1 chars2 0)))

(define (delete-at pos lst)
  (cond [(= 0 pos) (cdr lst)]
        [else (cons (car lst)
                    (delete-at (- pos 1) (cdr lst)))]))

(let ([firststr null]
      [secondstr null])
  (for-each (lambda (str)
              (for-each (lambda (otherstr) (if (= 1 (string-levenshtein str otherstr))
                                               (begin
                                                 (set! firststr str)
                                                 (set! secondstr otherstr))
                                               null))
                        input))
            input)
  (let ([pos (get-pos-different-char firststr secondstr)])
    (list->string (delete-at pos (string->list firststr)))))
